combos {
        compatible = "zmk,combos";
                      
      COMBO(fnOne, &kp F1, 1 11)
      COMBO(fTwo, &kp F2, 2 11)
      COMBO(fThree, &kp F3, 3 11)
      COMBO(fFour, &kp F4, 4 11)
      COMBO(fFive, &kp F5, 5 11)
      COMBO(fSix, &kp F6, 6 11)
      COMBO(fSeven, &kp F7, 7 0)
      COMBO(fEigth, &kp F8, 8 0)
      COMBO(fNine, &kp F9, 9 0)
      COMBO(fTen, &kp F10, 10 0)
      COMBO(fEleven, &kp F11, 11 0)
      COMBO(btZero, &bt BT_SEL 0, 0 11 24 35)
      COMBO(btOne, &bt BT_SEL 1, 1 10 25 34)
      COMBO(btTwo, &bt BT_SEL 2, 2 9 26 33)
      COMBO(btThree, &bt BT_SEL 3, 3 27 8 32)
      COMBO(btFour, &bt BT_SEL 4, 4 28 7 31)
      COMBO(btClr, &bt BT_CLR, 5 6 29 30)
      COMBO(btOut, &out OUT_TOG, 17 18)
      COMBO(briDn, &kp C_BRI_DN, 0 1)      
      COMBO(briUP, &kp C_BRI_UP, 1 2)
      COMBO(volDn, &kp C_VOL_DN, 9 10)
      COMBO(volUp, &kp C_VOL_UP, 10 11)
      COMBO(mut, &kp C_MUTE, 9 10 11)
      COMBO(space, &kp SPACE, 29 30)
      COMBO(dfltLyr, &to 0, 22 23)
      COMBO(numLyr, &to 1, 34 35)
    };


keymap {
    compatible = "zmk,keymap";	

  default_layer {
    bindings = <

&tdTabE     &tdQN1       &tdWN2      &tdEN3      &tdRN4      &tdTN5       &tdYN6       &tdUN7      &tdIN8      &tdUpON9       &tdPN0        &tdDelBslh
&sk LSHFT   &tdAMinus    &tdSPlus    &kp D    &mt SPACE F  &kp G    &kp H    &mt SPACE J  &tdLeftKRight   &tdDownLSemi    &tdRightAposGrave   &mt CAPS RET
&kp LCTRL   &kp LALT     &kp LGUI    &tdCXZ     &mt SPACE V   &mt SPACE B  &mt SPACE N  &mt SPACE M  &tdCommaLbkt  &tdDotBrkt   &kp FSLH   &sk RSHFT 

            >;
        };	

  Num_layer {
    bindings = <

&tdTabE     &kp N1         &kp N2        &kp N3     &kp N4      &kp N5      &kp N6      &kp N7      &kp N8      &mt UP N9        &kp N0        &kp DEL
&sk LSHFT   &kp MINUS     &kp EQUAL    &kp SPACE    &kp SPACE    &kp DOWN    &kp UP      &kp SPACE   &kp LEFT   &tdDownLSemi     &tdRightAposGrave   &kp RETURN	
&kp LCTRL   &kp LALT       &kp LGUI 	   &tdCXZ     &kp SPACE   &kp SPACE   &kp SPACE   &kp SPACE   &tdCommaLbkt  &tdDotBrkt    &kp FSLH     &sk RSHFT 

    >;
  };
 };
